{
  "name": "n8n-nodes-eigenlayer",
  "version": "1.0.0",
  "description": "n8n community node for EigenLayer",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "dist/nodes/EigenLayer/EigenLayer.node.js",
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.1.4",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "format": "prettier nodes credentials --write",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "n8n-community-node-package",
    "eigenlayer",
    "ethereum",
    "restaking",
    "blockchain"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/EigenLayerApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/EigenLayer/EigenLayer.node.js"
    ]
  },
  "dependencies": {
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.1.2",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}