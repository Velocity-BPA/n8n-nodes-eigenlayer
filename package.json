{
  "name": "n8n-nodes-eigenlayer",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for EigenLayer restaking protocol providing 9 resources and 80+ operations for smart contract integration with deposits, delegation, rewards, and slashing.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-eigenlayer.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-eigenlayer#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-eigenlayer/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "eigenlayer",
    "ethereum",
    "restaking",
    "defi",
    "blockchain",
    "web3",
    "smart-contracts",
    "avs",
    "delegation",
    "staking",
    "liquid-staking"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/EthereumRpc.credentials.js",
      "dist/credentials/EthereumWallet.credentials.js"
    ],
    "nodes": [
      "dist/nodes/EigenLayer/EigenLayer.node.js",
      "dist/nodes/EigenLayer/EigenLayerTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && npm run copy:icons",
    "copy:icons": "mkdir -p dist/nodes/EigenLayer && cp -f nodes/EigenLayer/*.svg dist/nodes/EigenLayer/ 2>/dev/null || true",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "ethers": "^6.13.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}
